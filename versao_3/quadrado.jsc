forma Quadrado {

    palavra cor;
    inteiro lado;
    inteiro ladoQuadrado;

    inteiro calculaArea(inteiro teste) {
        inteiro ladoQuadrado = lado * lado;
        retorno ladoQuadrado;
    }

    inteiro calcularPerimetro() {
        inteiro ladoQuadrado;   
        inteiro perimetro = lado * 4;
        retorno perimetro;
    }

}