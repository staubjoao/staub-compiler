forma Quadrado {

    palavra cor;
    inteiro lado;

    inteiro calculaArea(inteiro teste, inteiro teste2, inteiro teste3) {
        inteiro ladoQuadrado = lado * lado;
        retorno ladoQuadrado;
    }

    inteiro calcularPerimetro() {
        inteiro perimetro = lado * 4;
        retorno perimetro;
    }

}