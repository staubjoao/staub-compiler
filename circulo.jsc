forma Circulo {

    palavra cor;
    decimal raio;
    decimal pi = 3.14;

    decimal calculaArea(inteiro teste, decimal teste2) {
        decimal area;
        area = pi * (raio * raio);
        retorno area;
    }

    decimal calcularCircuferencia() {
        decimal circuferencia = 2 * raio * pi;
        imprime("teste");
        retorno circuferencia;
    }
}